<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PhoToCategoryScreen">




    <!-- Add more ImageView and TextView pairs here for the 2x3 grid -->

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Photos"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.152"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.08" />

    <View
        android:id="@+id/divider"
        android:layout_width="343dp"
        android:layout_height="7dp"
        android:background="?android:attr/listDivider"
        android:foreground="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.647"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.154" />

    <TableLayout
        android:layout_width="386dp"
        android:layout_height="604dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="10dp"
        android:background="#B6B6B2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider">

        <View
            android:layout_width="match_parent"
            android:layout_height="16dp" />

        <TableRow
            android:id="@+id/spaces"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/rowImageView6"
                android:layout_width="184dp"
                android:layout_height="103dp"
                android:src="@drawable/spaces" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:textSize="30dp"
                android:textStyle="bold"
                android:padding="8dp"
                android:text="Spaces"
                android:textColor="@color/black" />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="16dp" />

        <TableRow
            android:id="@+id/bikes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/rowImageView"
                android:layout_width="184dp"
                android:layout_height="103dp"
                android:src="@drawable/bikess" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="8dp"
                android:textSize="30dp"
                android:textStyle="bold"
                android:text="Bikes"
                android:textColor="@color/black" />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="16dp" />

        <TableRow
            android:id="@+id/mountain"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/rowImageView2"
                android:layout_width="184dp"
                android:layout_height="103dp"
                android:src="@drawable/mountain" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="8dp"
                android:textSize="30dp"
                android:textStyle="bold"
                android:text="Mountain"
                android:textColor="@color/black" />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="16dp" />

        <TableRow
            android:id="@+id/forest"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/rowImageView3"
                android:layout_width="184dp"
                android:layout_height="103dp"
                android:src="@drawable/forest" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="8dp"
                android:textSize="30dp"
                android:textStyle="bold"
                android:text="Forests"
                android:textColor="@color/black" />
        </TableRow>

        <View
            android:layout_width="match_parent"
            android:layout_height="16dp" />

        <TableRow
            android:id="@+id/cars"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/rowImageView4"
                android:layout_width="184dp"
                android:layout_height="103dp"
                android:src="@drawable/cars" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:textSize="30dp"
                android:textStyle="bold"
                android:padding="8dp"
                android:text="Cars"
                android:textColor="@color/black" />
        </TableRow>


    </TableLayout>


</androidx.constraintlayout.widget.ConstraintLayout>


